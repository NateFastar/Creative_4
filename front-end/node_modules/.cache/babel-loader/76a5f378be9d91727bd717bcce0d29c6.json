ntent":["<template>\r\n\t<div style=\"width: 95%\">\r\n\t\t<h1><br/>Current Collaborations</h1>\r\n\t\t<center><div class=\"alert alert-danger\" role=\"alert\" v-show=\"this.$root.$data.user==null\" style=\"width: 50%\">\r\n                You're not signed in! You can't collaborate without first signing in or creating a collaboration account!\r\n            </div></center>\r\n\t\t<div class=\"wrapper\">\r\n\t\t\t<div class=\"search\">\r\n\t\t\t\t<form class=\"pure-form\">\r\n\t\t\t\t\t<i class=\"fas fa-search\"></i><input v-model=\"searchText\" />\r\n\t\t\t\t</form>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t<h2 v-if=\"accepts.length > 0\"><br/>Accepted Collaborations</h2>\r\n\t\t<AcceptList :accepts=\"accepts\" />\r\n\t\t<h2 v-if=\"rejects.length > 0\"><br/>Ignored Collaborations</h2>\r\n\t\t<RejectList :rejects=\"rejects\" />\r\n\t\t<p v-if=\"projects.length==0\">Nothing here, sorry!</p>\r\n\t\t<h2 v-else>Active Collaborations</h2>\r\n\t\t<ProjectList :projects=\"projects\" />\r\n\t\t\r\n\t\t<div v-show=\"this.$root.$data.user!=null\">\r\n          <p>Add your own project\r\n          <button @click=\"setCreating\" class=\"pure-button button-xsmall\">\r\n            <i class=\"fas fa-plus\" />\r\n          </button></p>\r\n        </div>\r\n        <form class=\"pure-form\" v-if=\"creating\" @submit.prevent=\"addProject\">\r\n          <legend>Describe your project for the community.</legend>\r\n          <fieldset>\r\n\t\t\t<label style=\"margin-top: 10px\">Project Title:</label>\r\n            <textarea v-model=\"title\"></textarea>\r\n\t\t\t<label style=\"margin-top: 10px\">Project Description:</label>\r\n            <textarea v-model=\"problem\"></textarea>\r\n\t\t\t<label style=\"margin-top: 10px\">Reward:</label>\r\n\t\t\t<textarea v-model=\"reward\"></textarea>\r\n            <br />\r\n            <button @click=\"cancelCreating\" class=\"pure-button space-right\">Cancel</button>\r\n            <button class=\"pure-button pure-button-primary\" type=\"submit\">Submit</button>\r\n          </fieldset>\r\n        </form>\r\n        \r\n    </div>\r\n</template>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.search {\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  width: 50%;\r\n}\r\n\r\nform {\r\n  display: table;\r\n  width: 100%;\r\n}\r\n\r\ni {\r\n  display: table-cell;\r\n  padding-left: 10px;\r\n  width: 1px;\r\n}\r\n\r\ninput {\r\n  display: table-cell;\r\n  font-size: 20px;\r\n  border: none !important;\r\n  box-shadow: none !important;\r\n  width: 100%;\r\n  height: 40px;\r\n}\r\ntextarea {\r\n  width: 100%;\r\n  max-width: 500px;\r\n}\r\n\r\nh3 {\r\n  font-size: 12px;\r\n  font-weight: normal;\r\n  background-color: #ccc;\r\n  padding: 10px 20px;\r\n}\r\n\r\nlabel {\r\n  background-color: #000;\r\n  color: white;\r\n  padding: 5px;\r\n  border-radius: 30px;\r\n  font-size: 12px;\r\n  margin-right: 10px;\r\n}\r\n\r\n\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport moment from 'moment';\r\nimport ProjectList from \"../components/ProjectList.vue\"\r\nimport RejectList from \"../components/RejectList.vue\"\r\nimport AcceptList from \"../components/AcceptList.vue\"\r\n\r\nexport default {\r\n  name: 'Projects',\r\n  components: {\r\n    ProjectList,\r\n\tRejectList,\r\n\tAcceptList\r\n  },\r\n  data() {\r\n    return {\r\n      searchText: '',\r\n      creating: false,\r\n      problem: '',\r\n      title: '',\r\n      reward: '',\r\n    }\r\n  },\r\n  created() {\r\n    this.getProjects();\r\n  },\r\n  computed: {\r\n    projects() {\r\n\t\tif(this.$root.$data.projects!=null)return (this.$root.$data.projects.filter(project => project.problem.toLowerCase().search(this.searchText) >= 0));\r\n\t\telse return this.$root.$data.projects;\r\n\t},\r\n\trejects() {\r\n\t\tif(this.$root.$data.projects!=null) return (this.$root.$data.projects.filter(project => project.problem.toLowerCase().search(this.searchText) >= 0)).filter(project => project.status == 2);\r\n\t\telse return null;\r\n\t},\r\n\taccepts() {\r\n\t\tif(this.$root.$data.projects!=null) return (this.$root.$data.projects.filter(project => project.problem.toLowerCase().search(this.searchText) >= 0)).filter(project => project.status == 1);\r\n\t\telse return null;\r\n\t}\r\n  },\r\n  methods: {\r\n    async getProjects() {\r\n      try {\r\n        let response = await axios.get(\"/api/projects\");\r\n        this.$root.$data.projects = response.data.projects;\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    time(d) {\r\n      return moment(d).format('D MMMM YYYY, h:mm:ss a');\r\n    },\r\n\tsetCreating() {\r\n      this.creating = true;\r\n    },\r\n    cancelCreating() {\r\n      this.creating = false;\r\n    },\r\n    async addProject() {\r\n      try {\r\n        await axios.post(\"/api/projects\", {\r\n          problem: this.problem,\r\n          title: this.title,\r\n          reward: this.reward\r\n        });\r\n        this.problem = \"\";\r\n\t\tthis.title = \"\";\r\n\t\tthis.reward = \"\";\r\n        this.creating = false;\r\n        this.getProjects();\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n"],"sourceRoot":"src/views"}]}